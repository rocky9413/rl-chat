{
  "name": "rl-studio",
  "version": "1.0.0",
  "description": "server",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node ./dist/server.js",
    "build": "webpack && babel server --out-dir dist",
    "dev-client": "webpack-dev-server --env.mode development --open",
    "dev-server": "nodemon --exec babel-node ./server/server.js",
    "docker-dev": "docker-compose -f docker-compose-dev.yml up",
    "docker-test": "docker-compose -f docker-compose-test.yml up",
    "format": "prettier --write \"src/**/*.{js,jsx,html}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --env.mode production",
    "test": "jest",
    "test-reducer": "npm run test -t reducer",
    "test-router": "npm run test -t router",
    "test-model": "npm run test -t model",
    "test-controller": "npm run test -t controller"
  },
  "keywords": [],
  "author": "RL",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "socket.io": "^2.3.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node"
  }
}
